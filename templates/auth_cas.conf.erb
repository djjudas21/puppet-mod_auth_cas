#
# mod_auth_cas is an Apache 2.0/2.2 compliant module that supports the
# CASv1 and CASv2 protocols
#

<IfModule !mod_ssl.c>
LoadModule ssl_module <%= @module_path %>/mod_ssl.so
</IfModule>

CASCertificatePath <%= @certificatepath %>
CASLoginURL <%= @loginurl %>
CASValidateURL <%= @validateurl %>
<% if @proxyvalidateurl %>
CASProxyValidateURL <%= @proxyvalidateurl %>
<% end %>
CASCookiePath <%= @path %>/cache/
CASVersion <%= @version %>
CASDebug <%= @debug %>
